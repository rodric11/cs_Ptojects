<Window x:Class="DictationaryParser.AdditionalWindows.ExcelCreatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DictationaryParser.AdditionalWindows"
        mc:Ignorable="d"
        Title="ExcelCreatorWindow" Height="190.877" Width="373.46">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Name="ExcelNameLabel" Content="Имя файла" Grid.Row="0" Grid.Column="0" Margin="10, 10"></Label>
        <TextBox x:Name="ExcelNameTextBox" Grid.Row="0" Grid.Column="1" Margin="12, 12"></TextBox>
        <Label Name="ExcelSheetLabel" Content="Имя листа" Grid.Row="1" Grid.Column="0" Margin="10, 10"></Label>
        <TextBox x:Name="ExcelSheetTextBox" Grid.Row="1" Grid.Column="1" Margin="12, 12"></TextBox>
        <Button x:Name="ConfirmButton" Content="Создать" Click="ConfirmButton_Click" Grid.Row="2" Grid.ColumnSpan="2" Margin="100 10"></Button>
    </Grid>
</Window>
